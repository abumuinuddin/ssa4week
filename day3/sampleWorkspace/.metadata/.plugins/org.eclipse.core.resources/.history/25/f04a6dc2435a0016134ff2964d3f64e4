package gov.ssa.iron.yard;

public class Stack{
private int[] store;
private int len;

	public int[] getStore() {
		return store;
	}
	
	public void setStore(int[] store) {
		this.store = store;
	}
	
	public int getLen() {
		return len;
	}
	
	public void setLen(int len) {
		this.len = len;
	}

	public Stack() {
	    this.store = new int[10];
	    this.len = 0;
	}
	

	 public void push(int i) 
	    {
	        if (top == stack.length)
	        {
	            extendStack();
	        }

	        stack[top]= i;
	        top++;
	    }

	    public int pop() 
	    {
	        top --;
	        return stack[top];
	    }
	
	public static void main(String[] args) {
		int newStackLen = 50;
	    Stack st = new Stack();
	    

	    for (int i = 0; i < 50; i++) {
	        st.push(i);
	    }
	
	    for (int i = 0; i < 20; i++) {
	        System.out.println(st.pop());
	    }
	}
	
}   

