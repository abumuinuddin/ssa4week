public class HellowWorld {
	

	public static void main(String[] args) {
		List<String> lines =;

		try {
			lines = Files.readAllLines(Paths.get("word.text"), StandardCharsets.UTF_8);
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
	}
}